[mysqld]
# Server ID (deve ser único)
server-id=1

# Habilitar binary logging para replicação
log_bin=mysql-bin
binlog_format=ROW

# Banco de dados para replicar
binlog_do_db=aula-db

# Configurações de performance
max_connections=200
innodb_buffer_pool_size=256M

# Character set
character-set-server=utf8mb4
collation-server=utf8mb4_unicode_ci